<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFrameworks>net4</TargetFrameworks>
    <!--<TargetFrameworks>netcoreapp3.0;net4</TargetFrameworks>-->
    <UseWPF>true</UseWPF>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>sn.snk</AssemblyOriginatorKeyFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/geoas/WpfExtendedToolkit</PackageProjectUrl>
    <RepositoryUrl>https://github.com/geoas/WpfExtendedToolkit</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageLicenseExpression>MS-PL</PackageLicenseExpression>
    <Authors>AGIS</Authors>
    <PackageId>GeoAS.Extended.Wpf.Toolkit</PackageId>
    <Version>3.5.0</Version>
    <AssemblyVersion>3.5.0.1</AssemblyVersion>
    <FileVersion>3.5.0.1</FileVersion>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System.ComponentModel.DataAnnotations" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="**\*.png" />
  </ItemGroup>
</Project>